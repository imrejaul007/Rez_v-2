{
  "name": "rtmn-execution-system",
  "version": "1.0.0",
  "description": "Zero-thinking execution system for RTMN development",
  "scripts": {
    "generate:models": "cd generators && npm run generate:models",
    "generate:sdk": "cd generators && npm run generate:sdk",
    "generate:all": "npm run generate:models && npm run generate:sdk",
    "test": "jest",
    "test:architecture": "jest tests/architecture",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "validate:schemas": "tsc --noEmit schemas/*.schema.ts",
    "validate:openapi": "for file in contracts/*.openapi.yaml; do npx @ibm/openapi-validator \"$file\"; done",
    "migration:generate": "echo 'Migration generator not implemented'",
    "migration:run": "echo 'Migration runner not implemented'",
    "precommit": "npm run lint && npm run type-check && npm run test:architecture"
  },
  "dependencies": {
    "sequelize": "^6.35.0",
    "pg": "^8.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "@ibm/openapi-validator": "^1.11.0",
    "pre-commit": "^1.2.2"
  },
  "pre-commit": [
    "precommit"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imrejaul007/Rez_v-2.git"
  },
  "keywords": [
    "execution-system",
    "zero-thinking",
    "productivity",
    "automation",
    "rtmn"
  ],
  "author": "Rabtul Team",
  "license": "PROPRIETARY"
}
